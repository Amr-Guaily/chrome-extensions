{
  "manifest_version": 3,
  "name": "Open extension API reference",
  "version": "1.0.0",
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://extension-tips.glitch.me/*"],
  "minimum_chrome_version": "102",
  "omnibox": {
    "keyword": "api"
  },
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  }
}
